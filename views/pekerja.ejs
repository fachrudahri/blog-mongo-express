<h1 class="mt-4">List Pekerja</h1>
<div class="row">
    <% data.forEach(function(list) { %>
    <div class="col-xs-6 col-md-4 col-lg-3">
        <div class="card mb-3">
            <img src="https://png.pngtree.com/png-vector/20190119/ourlarge/pngtree-back-cartoon-cartoon-worker-worker-png-image_476449.jpg"
                class="card-img-top" alt="...">
            <div class="card-body">
                <h3 class="card-text"><%= list.name %> <br></h3>
                <p><%= list.email %></p>
            </div>
        </div>
    </div>
    <% }) %>
</div>